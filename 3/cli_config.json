{
  "batch_size": 100,
  "max_concurrent": 10,
  "backup_dir": "./backups",
  "import_dir": "./imports",
  "scripts_dir": "./scripts",
  "temp_dir": "./temp",
  "log_level": "INFO",
  "log_file": "admin_cli.log",
  "log_rotation": true,
  "max_log_size": "10MB",
  "connection_timeout": 30,
  "query_timeout": 300,
  "retry_attempts": 3,
  "backup_retention_days": 30,
  "compression_enabled": true,
  "include_metadata": true,
  "skip_duplicates": true,
  "validate_data": true,
  "auto_fix_issues": false,
  "script_timeout": 3600,
  "allow_dangerous_scripts": false,
  "script_log_output": true,
  "require_confirmation": true,
  "dry_run_default": false,
  "max_batch_delete": 1000,
  "email_notifications": false,
  "webhook_url": null,
  "slack_webhook": null,
  "memory_limit": "1GB",
  "temp_cleanup": true
}